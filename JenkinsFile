            steps {
                cmakeBuild buildDir: 'build', buildType: 'Debug', cleanBuild: true, installation: 'InSearchPath', label: 'pattern_C', sourceDir: 'Pattern/build/CMakeLists.txt', steps: [[args: 'clean all']]
            }
        }
        stage ('test') {
            steps {
                echo 'testing'
            }
        }
    }
}
